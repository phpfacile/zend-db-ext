{
    "name": "phpfacile/zend-db-ext",
    "description": "A zend-db extension",
    "keywords": ["zend-db", "extension", "now"],
    "homepage": "https://github.com/phpfacile/zend-db-ext",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Facile!",
            "homepage": "https://www.phpfacile.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "zendframework/zend-db": "^2.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "cs-check": "phpcs -s src",
        "cs-check-psr": "phpcs -s --standard=PSR1,PSR2 src",
        "cs-check-pear": "phpcs -s --standard=PEAR src",
        "cs-fix": "phpcbf src"
    },
    "autoload": {
        "psr-4": {
          "PHPFacile\\Zend\\Db\\": "src/"
        }
    }
}
